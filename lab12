		if (k % 12 > 2 and k % 12 < 6) cout << "черного ";
		else cout << "черной ";
		break;
	}
	switch (k % 12) {
	case (0):
		cout << "свиньи";
		break;
	case (1):
		cout << "крысы";
		break;
	case (2):
		cout << "коровы";

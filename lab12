		cout << "Ошибка";
		return 0;
	}
	k -= 3;
	if (k <= 0) k += 60;
	cout << "Год ";
	switch ((k-1)% 60 / 12) {
	case (0):
		if (k % 12 > 2 and k % 12 < 6) cout << "зелёного ";
		else cout << "зелёной ";
		break;
	case (1):
		if (k % 12 > 2 and k % 12 < 6) cout << "красного ";
		else cout << "красной ";
		break;

#include <iostream> 
#include <cmath>
using namespace std;
int main()
{
	setlocale(0, "");
	int a, a1, a2, b, i, n, s, k, z;
	double p, d, s1, s2;
	cout << "Введите номер задания" << "\n";
	cin >> z;
	if (z == 1) {
		cout << "Введите целые положительные числа A и B (A < B):\n";
		cin >> a;
		cin >> b;
		if (a >= b) {
			cout << "Ошибка";
		}
		else {
			for (a; a <= b; a++) {
				for (i = 1; i <= a; i++) {
					cout << a;
				}
				cout << "\n";
			}
		}
	}
	else if (z == 2) {
		cout << "Введите положительные числа A и B (A > B):\n";
		cin >> a;
		cin >> b;
		if (a <= b) {
			cout << "Ошибка";
		}
		else {
			while (a > b) {
				a -= b;
			}
			cout << "Длина незанятой части отрезка А равна " << a;
		}
	}
	else if (z == 3) {
		cout << "Введите целое число N (> 1):\n";
		cin >> n;
		s = 0;
		for (i = 1; s < n; i++) {
			s += i;
		}
		k = i - 1;
		cout << "Число К равно " << k << "\n";
		cout << "Сумма чисел от 1 до " << k << " равна " << s;
	}
	else if (z == 4) {
		cout << "Введите процент Р (0 < P < 25):\n";
		cin >> p;
		d = 1 + 0.01 * p;
		s2 = 1000;
		for (s1 = 1000, k = 1; s2 <= 1100; k++) {
			s2 = s1 * pow(d, k);
		}
		cout << "Найденное количество месяцев " << k << "\n";
		cout << "Итоговый размер вклада " << s2;
	}
	else if (z == 5) {
		cout << "Введите целые положительные числа A и B:\n";
		cin >> a;
		cin >> b;
		while ((a != 0) and (b != 0)) {
			if (a > b) {
				a = a % b;
			}
			else b = b % a;
		}
		cout << "НОД = " << a + b;
	}
	else if (z == 6) {
		cout << "Введите целое число N (> 1):\n";
		cin >> n;
		a1 = 1;
		a2 = 1;
		a = 0;
		k = 2;
		while (a < n) {
			k++;
			a = a2 + a1;
			a2 = a1;
			a1 = a;
		}
		if (n == a)
			cout << "Порядковый номер числа Фибоначчи: " << k + 1;
		else
			cout << "Не является числом Фибоначчи";
	}
	else if (z > 6) {
		cout << "Такого задания нет \n";
	}
}
